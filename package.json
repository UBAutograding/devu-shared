{
  "name": "devu-shared-modules",
  "version": "1.0.0",
  "description": "Autograding shared modules",
  "scripts": {
    "test": "jest --passWithNoTests",
    "clean": "rimraf build/*",
    "lint": "tsc",
    "build": "npm-run-all clean lint",
    "format": "prettier --write \"./**/*.{js,ts,json,md}\"",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./**/*.{js,ts,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rmraf": "^1.0.3",
    "typescript": "^4.3.2",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3"
  },
  "dependencies": {}
}
